class BankAccount:
  def __init__(self, fName, lName, balance, accountID, accountDigits):
      self.balance = balance
      self.accountID = accountID
      self.fName = fName
      self.lName = lName
      self.accountDigits = accountDigits

  def account(self):
     if len(self.accountDigits) != 6:
        return False
     if not self.accountDigits.isdigit():
        return False
     self.accountID = f"{self.fName[0]}{self.lName[0]}{self.accountDigits}"
       
    




